{
  "name": "dfmaster",
  "displayName": "dfmaster",
  "description": "This is a vscode extension that reads files into dataframes/tables.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.84.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "dfmaster.preview",
        "title": "Preview File To DataFrame",
        "category": "Developer"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "webpack --mode production --config webpack.config.ts",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/preset-react": "^7.7.4",
    "@types/mocha": "^10.0.3",
    "@types/node": "^20.9.0",
    "@types/react": "^16.14.35",
    "@types/react-dom": "^16.9.4",
    "@types/vscode": "^1.84.0",
    "@types/webpack": "^4.41.0",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "@vscode/test-electron": "^2.3.6",
    "babel-loader": "^8.0.6",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.4.0",
    "eslint": "^8.52.0",
    "glob": "^10.3.10",
    "mocha": "^10.2.0",
    "style-loader": "^1.0.2",
    "ts-loader": "^9.4.2",
    "ts-node": "^8.5.4",
    "typescript": "^5.2.2",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1"
  },
  "dependencies": {
    "concurrently": "^8.2.2",
    "path": "^0.12.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.12.0"
  }
}
